let
 var x: Integer;
 var y: Integer
in
 begin
 getint (var x);
 getint (var y);
 ! sum of x and y
 putint (x); put ('+'); putint (y); put ('='); putint (x+y);
 put(' ');
 ! product of x and y
 putint (x); put ('*'); putint (y); put ('='); putint (x*y); put(' ');
 ! "same" if x and y are the same, otherwise nothing
 if x=y
 then
 begin
  put('s'); put('a'); put('m'); put('e')
 end
 else;
 end